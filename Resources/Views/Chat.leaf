<!doctype html>
<html lang="en">
    <head>
        <script src="Scripts/main.js"></script>
        <style>
            .wrapper {
                display: grid;
                grid-template-rows: 300px 50px;
                grid-gap: 10px;
                background-color: #fff;
                color: #444;
            }
            .input-wrapper {
                display: grid;;
                grid-template-columns: 300px 50px;
                grid-gap: 10px;
                background-color: #fff;
                color: #444;
            }
        </style>
    </head>
    <body>
        <div class="wrapper">
            <output class="output" id="output">
                any
            </output>
            <div class="input-wrapper">
                <input id="messageInput"></input>
                <button onclick="onSendMessage()">Send</button>
            </div>
        </div>
    </body>
</html>
